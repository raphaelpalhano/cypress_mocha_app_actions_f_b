{
  "name": "cucumber-preprocessor",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prettier-format": "prettier --config .prettierrc '**/*.js' --write",
    "cy:firefox": "cypress run --browser firefox",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:api": "npx cypress run --env grepTags='@api'",
    "cy:open:api": "npx cypress open --env grepTags='@api'",
    "cy:run:front": "npx cypress run --env grepTags='@frontend'",
    "cy:open:front": "npx cypress open --env grepTags='@frontend'",
    "cy:clear": "node ./cypress/support/utils/clear.js",
    "lint": "eslint . --ext .js",
    "commit": "git-cz",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Raphael Angel",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@typescript-eslint/parser": "^5.40.0",
    "commitizen": "^4.2.5",
    "cypress-localstorage-commands": "^1.7.0",
    "cypress-multi-reporters": "^1.5.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.22.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.26.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "mocha": "^10.0.0",
    "mochawesome": "^7.1.3",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "@testing-library/cypress": "^8.0.3",
    "ajv": "^8.11.0",
    "cy-verify-downloads": "^0.1.11",
    "cypress": "^10.8.0",
    "cypress-failed-log": "^2.10.0",
    "cypress-file-upload": "^5.0.8",
    "cypress-grep": "^3.0.2",
    "cypress-mochawesome-reporter": "^3.2.0",
    "cypress-real-events": "^1.7.1",
    "cypress-xpath": "^2.0.1",
    "dotenv": "^16.0.1",
    "fs-extra": "^10.1.0",
    "jsonschema": "^1.4.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.4"
  },
  "lint-staged": {
    "*.js": [
      "npm run prettier-format",
      "npm run lint --fix"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
